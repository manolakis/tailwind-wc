{
  "name": "@manolakis/css4wc",
  "version": "0.0.0",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/manolakis/css4wc.git"
  },
  "author": {
    "name": "Manuel Martin",
    "email": "manuel.martin@gmail.com",
    "url": "https://github.com/manolakis"
  },
  "scripts": {
    "build": "rollup -c rollup.config.js",
    "debug": "wtr --watch --config web-test-runner.config.mjs",
    "format": "run-p format:*",
    "format:eslint": "eslint --ext .js,.html . --fix",
    "format:prettier": "prettier \"**/*.{js,json,md}\" --write",
    "lint": "run-p lint:*",
    "lint:eclint": "git ls-files | xargs eclint check",
    "lint:eslint": "eslint --ext .js,.html .",
    "lint:markdownlint": "git ls-files '*.md' | xargs markdownlint --ignore '{.github/**/*.md,.changeset/*.md,**/CHANGELOG.md}'",
    "lint:prettier": "prettier \"**/*.{js,json,md}\" --list-different || (echo '↑↑ these files are not prettier formatted ↑↑' && exit 1)",
    "release": "changeset publish",
    "start": "rocket start",
    "storybook": "wds -c wds-storybook.config.js --watch",
    "storybook:build": "build-storybook",
    "test": "wtr --coverage"
  },
  "files": [],
  "dependencies": {
    "singleton-manager": "^1.4.2"
  },
  "devDependencies": {
    "@changesets/cli": "^2.16.0",
    "@commitlint/cli": "^12.1.1",
    "@commitlint/config-conventional": "^12.1.1",
    "@open-wc/eslint-config": "^4.3.0",
    "@open-wc/testing": "^2.5.33",
    "@rocket/cli": "^0.6.2",
    "@rocket/launch": "^0.4.0",
    "@rollup/plugin-node-resolve": "^13.0.0",
    "@web/dev-server": "^0.1.15",
    "@web/dev-server-import-maps": "0.0.5",
    "@web/dev-server-storybook": "^0.3.5",
    "@web/test-runner": "^0.12.20",
    "@web/test-runner-commands": "^0.4.4",
    "@web/test-runner-junit-reporter": "^0.4.2",
    "@web/test-runner-mocha": "^0.7.2",
    "@web/test-runner-playwright": "^0.8.5",
    "eclint": "^2.8.1",
    "eslint-config-prettier": "^8.2.0",
    "husky": "^6.0.0",
    "lint-staged": "^10.5.4",
    "lit-element": "^2.4.0",
    "markdownlint-cli": "^0.27.1",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.2.1",
    "prettier-plugin-package": "^1.3.0",
    "remark-extend": "^0.3.0",
    "rollup": "^2.50.5",
    "rollup-plugin-terser": "^7.0.2",
    "sinon": "^11.1.1"
  },
  "lint-staged": {
    "*": [
      "eclint"
    ],
    "*.{js,cjs,html}": [
      "eslint --fix"
    ],
    "*.{js,json,md}": [
      "prettier --write"
    ],
    "*.md": [
      "markdownlint --ignore '{.github/**/*.md,.changeset/*.md,**/CHANGELOG.md}'"
    ]
  },
  "prettier": {
    "printWidth": 100,
    "singleQuote": true,
    "arrowParens": "avoid",
    "trailingComma": "all"
  }
}
